<Window x:Class="VITP._10_VacasWPF.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="DgVacas" ItemsSource="{Binding ListVacas}" AutoGenerateColumns="True" Margin="0,0,0,86" IsReadOnly="False" AutoGenerateColumns="True" Margin="0,0,0,202" CellEditEnding="dgVacas_CellEditEnding"/>
        <Button x:Name="BtnNewVaca" Content="Nueva Vaca" HorizontalAlignment="Left" Margin="64,379,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button x:Name="BtnEliminar" Content="Eliminar" HorizontalAlignment="Left" Margin="171,379,0,0" VerticalAlignment="Top" Click="BtnEliminar_Click"/>
    </Grid>
</Window>
