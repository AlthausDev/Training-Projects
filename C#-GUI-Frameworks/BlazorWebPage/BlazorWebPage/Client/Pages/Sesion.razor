    Responsive="true">

    <GridColumn TItem="User" HeaderText="Id" PropertyName="Id" SortKeySelector="item => item.Id" IsDefaultSortColumn="true">
    @context.Id
    </GridColumn>

    <GridColumn TItem="User" HeaderText="UserName" PropertyName="UserName" SortKeySelector="item => item.UserName">
    @context.UserName
    </GridColumn>

    <GridColumn TItem="User" HeaderText="Nombre" PropertyName="Nombre" SortKeySelector="item => item.Nombre">
